<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="AP" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Main">
   <children>
      <VBox fx:id="startBox" layoutX="100.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0">
         <children>
            <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onEnterClicked" prefHeight="211.0" prefWidth="353.0" style="-fx-background-color: tranperent; -fx-font-size: 25; -fx-font-weight: bold;" text="Вход">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#onExitClicked" prefHeight="244.0" prefWidth="353.0" style="-fx-background-color: transperent; -fx-font-size: 25; -fx-font-weight: bold;" text="Выход" />
         </children>
      </VBox>
   </children>
</AnchorPane>
